                            <ComboBox Background="Transparent" ItemsSource="{Binding GenderList}" SelectedItem="{Binding Volunteer.Gender,Mode=TwoWay}" MinWidth="296" Padding="10" PlaceholderText="Gender" BorderThickness="0 0 0 2" Margin="10 0 0 0" BorderBrush="{StaticResource YellowLightBrush}" Foreground="{StaticResource GrayLightBrush}" FontSize="16"   />
                    </StackPanel>

                </StackPanel>
                <StackPanel Grid.Row="8" Margin="10 20">

                    <Button MaxWidth="400" Command="{Binding GoToPageTwo}"  Padding="10" Template="{StaticResource ButtonRoundedTemplate}"  HorizontalAlignment="Stretch" Content="Next" Foreground="{StaticResource WhiteLightBrush}" Background="#FFC2A30D" FontSize="16"/>
                    
                        <Button  MaxWidth="400" Command="{Binding GoToBackPage}"  Padding="10" Template="{StaticResource ButtonRoundedTemplate}"  HorizontalAlignment="Stretch" Content="Back" Foreground="{StaticResource WhiteLightBrush}" Background="#FFC2A30D" FontSize="16"/>
                    </StackPanel>
            </Grid>
        </Grid>
    </Grid>
    </ScrollViewer>
</UserControl>
